#include<stdio.h>
void reorder(int a[],int n)
int k=0;
for(int i=0;i<n;i++)
if(a[i]!=0)
{
    a[k++]=a[i];
}
}
for(int i=k;i<n;i++)
{
    a[i]=0;
}
}
int main(void)
{
    int a[]={6,0,8,2,3,0,4,0,1};
    int n=size of(a)/ size of (a[0]);
    reorder(a,n)
    for(int i=0;i<n;i++)
    {
        printf("%d",a[i]);
    }
    return 0;
    }
    


    
    


